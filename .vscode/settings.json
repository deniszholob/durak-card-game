{
  "files.exclude": {
    "**/.babelrc": false,
    "**/*.d.ts": false,
    "documentation.json": false,
    "angular.json": false,
    "**/.browserslistrc": false,
    "**/environments": false,
    "**/src/main.ts": false,
    "**/polyfills.ts": false,
    "**/test-setup*": false,
    "**/.eslint*": false,
    "**/jest*": false,
    "**/tsconfig*": false,
    "**/coverage/": false,
    "**/dist/": false,
    "**/screenshots/": false,
    "**/tools/": false,
    "**/.editorconfig": false,
    "**/.gitignore": false,
    "**/.gitkeep": false,
    "**/.npmrc": false,
    "**/.prettierignore": false,
    "**/.prettierrc": false,
    "**/decorate-angular-cli.js": false,
    "**/FUNDING.yml": false,
    "**/LICENSE": false,
    "**/readme-nx.md": false,
    "**/package-lock.json": false,
    "**/.angular": false,
    "**/.github": false,
    "**/.husky": false,
    "**/node_modules": false
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.renderWhitespace": "all",
  "editor.rulers": [80],
  "explorer.sortOrderLexicographicOptions": "lower", // Case sensitive file ordering in tree
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.activeIconPack": "angular_ngrx",
  "material-icon-theme.folders.associations": {
    "web": "client",
    "web-e2e": "test",
    "options": "config",
    "directives": "components",
    "enums": "mjml",
    "models": "mjml",
    "forms": "docs",
    "wrappers": "hook",
    "document": "",
    "workflows": "github",
    "formly": "scala"
  },
  "material-icon-theme.files.associations": {
    "*.interceptor.ts": "apollo",
    "*.events.ts": "tcl",
    "*.data.ts": "database",
    "*.model.ts": "mjml",
    "*.enum.ts": "database",
    "*.const.ts": "database",
    "model.ts": "mjml",
    "index.ts": "url",
    "public-api.ts": "url",
    "models.ts": "url",
    "services.ts": "url"
  },
  "cSpell.words": ["durak", "titlecase"],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "svg.preview.background": "transparent"
}
