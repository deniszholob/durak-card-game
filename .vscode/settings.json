{
  "files.exclude": {
    "**/.browserslistrc": true,
    "**/environments": true,
    "**/src/main.ts": true,
    "**/polyfills.ts": true,
    "**/test-setup*": true,
    "**/.eslint*": true,
    "**/jest*": true,
    "**/tsconfig*": true,
    "**/coverage/": true,
    "**/dist/": true,
    "**/screenshots/": true,
    "**/tools/": true,
    "**/.editorconfig": true,
    "**/.gitignore": true,
    "**/.gitkeep": true,
    "**/.npmrc": true,
    "**/.prettierignore": true,
    "**/.prettierrc": true,
    "**/decorate-angular-cli.js": true,
    "**/FUNDING.yml": true,
    "**/LICENSE": true,
    "**/readme-nx.md": true,
    "**/.angular": true,
    "**/.github": true,
    "**/.husky": true,
    "**/node_modules": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.renderWhitespace": "all",
  "editor.rulers": [80],
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.activeIconPack": "angular_ngrx",
  "material-icon-theme.folders.associations": {
    "web": "client",
    "web-e2e": "test",
    "options": "config",
    "directives": "components",
    "enums": "mjml",
    "models": "mjml",
    "forms": "docs",
    "wrappers": "hook",
    "document": "",
    "workflows": "github",
    "formly": "scala"
  },
  "material-icon-theme.files.associations": {
    "*.interceptor.ts": "apollo",
    "*.events.ts": "tcl",
    "*.data.ts": "database",
    "*.model.ts": "mjml",
    "*.enum.ts": "database",
    "*.const.ts": "database",
    "model.ts": "mjml",
    "index.ts": "url",
    "public-api.ts": "url",
    "models.ts": "url",
    "services.ts": "url"
  },
  "cSpell.words": ["durak"],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "svg.preview.background": "transparent"
}
