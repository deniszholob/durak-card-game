{
  "name": "durak-card-game",
  "version": "0.0.0",
  "license": "GPL-3",
  "author": "Denis Zholob",
  "description": "Durak Card Game",
  "private": true,
  "engines": {
    "node": "18.12.1",
    "npm": "8.18.2"
  },
  "scripts": {
    "ng": "ng",
    "i": "npm i",
    "start": "ng serve",
    "test": "jest --coverage",
    "view-coverage": "cd coverage/lcov-report && index.html",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "ng run durak-card-game:storybook",
    "storybook:run": "npm run docs:json && start-storybook -p 6006",
    "storybook:build": "npm run docs:json && build-storybook",
    "version-bump": "npm version patch --no-git-tag-version && git add package.json package-lock.json",
    "prepare": "husky install",
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --cache --fix"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@angular/animations": "13.3.0",
    "@angular/common": "13.3.0",
    "@angular/compiler": "13.3.0",
    "@angular/core": "13.3.0",
    "@angular/forms": "13.3.0",
    "@angular/platform-browser": "13.3.0",
    "@angular/platform-browser-dynamic": "13.3.0",
    "@angular/router": "13.3.0",
    "@fortawesome/fontawesome-free": "6.1.1",
    "rxjs": "7.5.0",
    "tslib": "2.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.3",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "13.3.3",
    "@angular/compiler-cli": "13.3.0",
    "@babel/core": "^7.20.2",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/angular": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "27.4.1",
    "@types/node": "12.11.1",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "autoprefixer": "10.4.5",
    "babel-loader": "^8.3.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-storybook": "0.6.7",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.0",
    "jest": "27.0.3",
    "jest-preset-angular": "11.1.2",
    "postcss": "8.4.12",
    "prettier": "2.6.2",
    "prettier-plugin-organize-attributes": "0.0.5",
    "tailwindcss": "3.0.24",
    "ts-jest": "27.0.3",
    "typescript": "4.6.2"
  }
}
